public class PatternStitch extends Stitch{
    private Stitch next;
    private Stitch previous;

    public PatternStitch(String name, String symbol, Stitch next, Stitch previous){
        this.name = name;
        this.symbol  = symbol;
        this.next = next;
        this.previous = previous;
    }

    public void setNext(Stitch stitch){
        this.next = stitch;
    }

    public Stitch getNext(){
        return this.next;
    }

    public void setPrevious(Stitch stitch){
        this.previous = stitch;
    }

    public Stitch getPrevious(){
        return this.previous;
    }
}